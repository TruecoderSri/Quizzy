{
  "version": 2,
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/routes/authRoutes/$1"
    },
    {
      "src": "/api/questions/(.*)",
      "dest": "/routes/questionRoutes/$1"
    },
    {
      "src": "/api/quizzes/(.*)",
      "dest": "/routes/quizRoutes/$1"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/routes/profileRoutes/$1"
    }
  ]
}
